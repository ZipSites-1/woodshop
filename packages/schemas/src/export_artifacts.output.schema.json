{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://woodshop.dev/schemas/export_artifacts.output.json",
  "title": "ExportArtifactsOutput",
  "type": "object",
  "additionalProperties": false,
  "properties": {
    "project_id": { "$ref": "./common.schema.json#/$defs/projectId" },
    "format": { "type": "string", "enum": ["pdf", "dxf", "svg"] },
    "artifact_path": {
      "type": "string",
      "description": "Relative path to the exported artifact"
    },
    "artifact_sha256": {
      "type": "string",
      "description": "Checksum of artifact contents"
    },
    "bytes_written": {
      "type": "integer",
      "minimum": 0,
      "description": "Number of bytes written to disk"
    },
    "seed": { "$ref": "./common.schema.json#/$defs/seedOutput" },
    "engine_versions": { "$ref": "./common.schema.json#/$defs/engineVersions" },
    "revision_id": { "$ref": "./common.schema.json#/$defs/revisionId" },
    "inputs_hash": { "$ref": "./common.schema.json#/$defs/inputsHash" }
  },
  "required": [
    "project_id",
    "format",
    "artifact_path",
    "artifact_sha256",
    "bytes_written",
    "seed",
    "engine_versions",
    "revision_id",
    "inputs_hash"
  ]
}
